@page "/"
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery AccessT
@namespace ExcelManagement.DxBlazor.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    var tokens = new TokenProvider
            {
                AccessToken = AccessT.GetAndStoreTokens(HttpContext).RequestToken
            };
}

<component type="typeof(App)" param-InitialState="tokens" render-mode="ServerPrerendered" />